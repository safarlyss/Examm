@using WebApplication1.Models;

@{
    ViewData["Title"] = "Edit";
}

@model Team
<div class="col-md-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">
            <h4 class="card-title">Edit</h4>

            <form class="forms-sample" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label asp-for="FullName">FullName</label>
                    <input asp-for="FullName" type="text" class="form-control" id="exampleInputUsername1" placeholder="FullName">
                    <span asp-validation-for="FullName"></span>
                </div>
                <div class="form-group">
                    <label asp-for="Profession"></label>
                    <input asp-for="Profession" type="text" class="form-control" id="exampleInputUsername1" placeholder="Profession">
                    <span asp-validation-for="Profession"></span>
                </div>

                <div class="form-group">
                    <label asp-for="File"></label>
                    <input asp-for="File" class="form-control" id="exampleInputUsername1">
                    <span asp-validation-for="File"></span>
                </div>

                <button type="submit" class="btn btn-primary me-2">Submit</button>
                <button class="btn btn-light"><a asp-action="Index" asp-controller="Settings"></a>Cancel</button>
            </form>
        </div>
    </div>
</div>

